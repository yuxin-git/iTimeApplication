<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="280dp"
    android:layout_height="170dp"
    android:orientation="vertical">


    <TextView
        android:id="@+id/text_view_dis_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="TextView"
        android:textSize="30dp"/>

    <TextView
        android:id="@+id/text_view_dis_date"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="TextView"
        android:textSize="25dp"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="80dp"
        >
        <LinearLayout
            android:id="@+id/linearLayout1"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="0dp"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/text_view_dis_day"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:textSize="19dp"
                android:gravity="center"
                android:text="Text" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="days" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="70dp"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/text_view_dis_hour"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:textSize="19dp"
                android:gravity="center"
                android:text="Text" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="hours" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout3"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="140dp"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/text_view_dis_munite"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:textSize="19dp"
                android:gravity="center"
                android:text="Text" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="munites" />

        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayout4"
            android:layout_width="70dp"
            android:layout_height="80dp"
            android:layout_marginTop="0dp"
            android:layout_marginLeft="210dp"
            android:orientation="vertical"
            >

            <TextView
                android:id="@+id/text_view_dis_second"
                android:layout_width="70dp"
                android:layout_height="50dp"
                android:textSize="19dp"
                android:gravity="center"
                android:text="Text" />
            <TextView
                android:layout_width="70dp"
                android:layout_height="30dp"
                android:textSize="17dp"
                android:gravity="center"
                android:text="seconds" />

        </LinearLayout>
    </FrameLayout>

</LinearLayout>
